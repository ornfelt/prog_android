<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/buttonThirdVideo"
        android:layout_width="185dp"
        android:layout_height="70dp"
        android:onClick="onClickThirdVideo"
        android:text="Third Video"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonSecondVideo"
        app:layout_constraintVertical_bias="0.39" />

    <Button
        android:id="@+id/buttonSecondVideo"
        android:layout_width="185dp"
        android:layout_height="70dp"
        android:onClick="onClickSecondVideo"
        android:text="Second Video"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/buttonFirstVideo"
        app:layout_constraintVertical_bias="0.19999999" />

    <TextView
        android:id="@+id/textViewYoutubeApp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="YoutubeApp!"
        android:textSize="36sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.110000014" />

    <Button
        android:id="@+id/buttonFirstVideo"
        android:layout_width="185dp"
        android:layout_height="70dp"
        android:onClick="onClickFirstVideo"
        android:text="First Video"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewYoutubeApp"
        app:layout_constraintVertical_bias="0.22000003" />

</androidx.constraintlayout.widget.ConstraintLayout>